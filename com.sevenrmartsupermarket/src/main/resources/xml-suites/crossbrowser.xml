<suite name="Cross Browser" thread-count="3" parallel="tests">
	<listeners>
		<listerner
			class-name="com.sevenrmartsupermarket.listeners.ExtentReporter">
		</listerner>
	</listeners>
	<test name="Run in Chrome">
		<parameter name="browser" value="Chrome">
		</parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
			<class name="com.sevenrmartsupermarket.tests.DashBoardTest">
			</class>
			<class name="com.sevenrmartsupermarket.tests.ManageNewsTest">
			</class>
		</classes>
	</test>
	<test name="Run in Edge">
		<parameter name="browser" value="Edge">
		</parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
			<class name="com.sevenrmartsupermarket.tests.DashBoardTest">
			</class>
			<class name="com.sevenrmartsupermarket.tests.ManageNewsTest">
			</class>
		</classes>
	</test>
	<test name="Run in Firefox">
		<parameter name="browser" value="Firefox">
		</parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
			<class name="com.sevenrmartsupermarket.tests.DashBoardTest">
			</class>
			<class name="com.sevenrmartsupermarket.tests.ManageNewsTest">
			</class>
		</classes>
	</test>
</suite>